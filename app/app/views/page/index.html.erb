<%= render "header" %>

<div class="container body">
  <p class="h1 tab-name">Latest Posts</p>
  <br>

    <% i = 0 %>
    <% @videos.each do |video| %>
      <% if i < 5 then %>
        <%= render partial: "video", locals: { video: video } %>
        <% i += 1%>
      <% else %>
        <% break %>
      <% end %>
    <% end %>
</div>
